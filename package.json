{
  "name": "@airfair/event-manager",
  "version": "1.0.0",
  "description": "A TypeScript utility class for efficient management of event listeners in web applications.",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "optimize": true,
      "includeNodeModules": true
    }
  },
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build && ./post-build.sh",
    "prepare": "pnpm build"
  },
  "keywords": ["Explicit Resource Management", "ESNext", "TypeScript"],
  "author": "Alexandre Moreau-Lemay",
  "license": "MIT",
  "dependencies": {
    "core-js": "3.35.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-explicit-resource-management": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@parcel/babel-preset-env": "^2.11.0",
    "@parcel/packager-ts": "2.11.0",
    "@parcel/transformer-typescript-types": "2.11.0",
    "@types/core-js": "^2.5.8",
    "parcel": "^2.11.0",
    "typescript": "^5.3.3"
  }
}
